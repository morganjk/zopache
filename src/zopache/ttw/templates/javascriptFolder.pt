
${structure: view.breadcrumbsManage()}

<a tal:attributes="href view.url(view.context) + '/aceedit'"
   tal:content="view.context.__name__"></a> ${view.context.title}<br>
<ul>
<span tal:repeat="item view.context.values()">
<li><a tal:attributes="href view.url(item) + '/manage'"
   tal:content="item.__name__"></a> ${view.context.title} </li>
</span>
</ul>
<table >

<tr tal:repeat="item view.context.flatten(view)" >
      <td tal:content="structure:item.count"> </td>
      <td tal:content="structure: u'&nbsp;&nbsp;' + item.line"></td> 
</tr>
</table>





